<%= @study_session.errors.full_messages %>
<%= form_with model: @study_session, local: true do |form|%>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <% collection_array = current_user.collections.map { |collection| [collection.name, collection.id] } %>
  <%= select(:study_session,:collection_id,collection_array) %>
  <%= time_field :study_session,:duration_start %>
  <%= time_field :study_session,:duration_end %>
  <%= form.submit "Submit", class:"" %>
<% end %>
