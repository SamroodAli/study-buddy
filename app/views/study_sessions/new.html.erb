<%= form_with model: @study_session, local: true do |form|%>
  <%= form.label :name %>
  <br>
  <%= form.text_field :name ,required: true%>
  <br>
  <%= form.label :collection %>
  <%= form.select :collection_id, @collections, :selected => @selected %>
  <br>
  <%= form.label :duration_start %>
  <%= time_field :study_session,:duration_start, required: true%>
  <br>
  <%= form.label :duration_end %>
  <br>
  <%= time_field :study_session,:duration_end ,required: true%>
  <br>
  <%= form.submit "Submit", class:"" %>
<% end %>
